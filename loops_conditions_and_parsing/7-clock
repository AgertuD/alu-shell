#!/usr/bin/env bash
#!/bin/bash

# Initialize hour counter
hour=0

# Loop through hours from 0 to 12
while [ $hour -le 12 ]
do
    # Initialize minute counter
    minute=1
    
    # Loop through minutes from 1 to 59
    while [ $minute -le 59 ]
    do
        echo "$hour:$minute"
        ((minute++))  # Increment the minute counter
    done

    ((hour++))  # Increment the hour counter
done | head -n 70  # Display the first 70 lines
